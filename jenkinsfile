pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                mvn clean
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
       
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                mvn package
            }
        }
    }
}
